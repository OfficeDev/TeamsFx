  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.0",
    "@types/tmp": "^0.2.6",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "dotenv": "^16.4.1",
    "dree": "^4.6.2",
    "fs-extra": "^11.2.0",
    "tmp": "^0.2.1"
  }
