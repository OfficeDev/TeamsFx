{
  "name": "teams-agent-poc",
  "publisher": "Microsoft",
  "version": "0.1.0",
  "displayName": "Teams Chat Agent",
  "description": "Copilot chat agent for Teams",
  "repository": {
    "type": "git",
    "url": "https://dev.azure.com/devdiv/DevDiv/_git/teamsagentPoc"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "extensionDependencies": [
    "github.copilot-chat"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "enabledApiProposals": [
    "chat",
    "chatAgents2",
    "chatAgents2Additions",
    "chatProvider",
    "chatRequestAccess",
    "interactive"
